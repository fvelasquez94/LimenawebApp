<!--FOR MENU-->
<script>
    var string = @Html.Raw(ViewBag.lstDepartments);

    console.log(string);
    string.forEach(function (element) {
        if (element == "Sales") {
            document.getElementById("dep_menu").style.display = "block";
        }
        if (element == "Management") {
            document.getElementById("manag_menu").style.display = "block";
        }
    });
</script>
<!--END-->
<!--FOR ACTIVE MENU-->
<script>

          var idMenu = "@ViewBag.menunameid";
    var menu = document.getElementById(idMenu);
    menu.classList.add("active");

    var idsubMenu = "@ViewBag.submenunameid";
    var submenu = document.getElementById(idsubMenu);
    submenu.classList.add("active");


</script>