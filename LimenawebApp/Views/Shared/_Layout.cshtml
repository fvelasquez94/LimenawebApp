                $("#tipobonificacion").val(0);
                $("#tipobonificacion").trigger("change");

            }






        }
        $("#tipobonificacion").on('change', function () {
            var value = $(this).val();

            if (value == 1) {
                $('#typeUnits').show();
                $('#typeCurrency').hide();
                $('#cantidadBonificar').val(0);
                $('#pricebon').val(0);
            } else if (value == 2) {
                $('#typeUnits').hide();
                $('#typeCurrency').show();
                $('#cantidadBonificar').val(0);
                $('#pricebon').val(0);
            }else if (value == 0) {
                $('#typeUnits').hide();
                $('#typeCurrency').hide();
                $('#cantidadBonificar').val(0);
                $('#pricebon').val(0);
            }


        });


        function guardarBonificacion() {


            $("#savebonbtn").attr("disabled", true);
            var pedido = "@ViewBag.docnum";
            var producto = $('#idproductoBon').val();
            var lineNum = $("#lineNumBon").val();
            var unidades = $('#cantidadBonificar').val();
            var tipobonificacion = $("#tipobonificacion").val();
            var precio = $('#pricebon').val();
            var userID = "@ViewBag.userID";
            var userName = "@ViewBag.userName";
            var uombon = $("#tipoUOM").val();
            var now = new Date();
            var nombreProducto = $('#productBon').val();
            var tipo = "";
            var closedTimes = @ViewBag.contadorClosed;
            var validando = 0;

            if (tipobonificacion == 1) {
                tipo = '<span class="badge text-success">QUANTITY</span>'

                if (unidades < 1) {
                    validando = 1;//No paso el filtro por lo tanto mandamos alerta
                }
            }
            else if (tipobonificacion == 2)
            {
                if (precio < 1) {
                    validando = 1;//No paso filtro de precio
                }
                tipo = '<span class="badge text-info">CURRENCY</span>'
            }

            if (validando == 1) {
                toastr.warning("Something went wrong. You must to set a valid value.", "Warning");
                $('#savebonbtn').attr("disabled", false);
            } else {

                document.getElementById("loader").style.display = "block";
                $.ajax
                    ({
                        url: '/Commercial/Save_bonification',
                        type: 'POST',
                        datatype: 'application/json',
                        contentType: 'application/json',
                        data: JSON.stringify({
                            pedido: pedido,
                            producto: producto,
                            tipobonificacion: tipobonificacion,
                            lineNum: lineNum,
                            precio: precio,
                            unidades: unidades,
                            date: now.toLocaleString("en-US", { hour12: true }),
                            userID: userID,
                            userName: userName,
                            uom: uombon,
                            timesClosed: closedTimes
                        }),
                        success: function (result) {

                            if (result == "SUCCESS") {
                                toastr.success("Data saved successfully.", "Success");
                                document.getElementById("loader").style.display = "none";
                                $('#savebonbtn').attr("disabled", false);

                                var t = $('#tbBonificaciones').DataTable();

                                t.row.add(["-",
                                    nombreProducto, tipo,
                                    unidades, precio, uombon,
                                    '<span class="badge text-warning">WAITING FOR APPROVAL</span>', '<span class="badge text-warning">TO VIEW ACTIONS, PLEASE REFRESH THE PAGE</span>'
                                ]).draw(false);

                                setTimeout(function () { $('#tbbonificaciones').modal('show') }, 500);
                                $('#nuevabonificacion').modal('hide');
                                $("#btnAsk").show();
                                $("#btnbackno").hide();


                            } else {
                                toastr.warning("Something went wrong.." + result, "Warning");
                                document.getElementById("loader").style.display = "none";
                                $('#savebonbtn').attr("disabled", false);

                            }


                        },
                        error: function () {
                            $('#savebonbtn').attr("disabled", false);

                            toastr.warning("Something went wrong..", "Warning");
                            document.getElementById("loader").style.display = "none";
                        },
                    });
            }






        }
    </script>



}
