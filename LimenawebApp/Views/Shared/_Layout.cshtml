                }

                $(this).closest('tr').find('.editCobertura_dePO').html(total.toFixed(2))
                //            //$(this).html('DEL');

                ///Monto PO
                var desc = 0;

                desc = $(this).closest('tr').find('.editCostoDescuento').text();

                var monto = desc * pedido;
