@{
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
    ViewBag.Title = "Distribuidora Limeña - Activity";

}
@helper OrderedListRetail(IList<LimenawebApp.Controllers.CommercialController.MyObj_tablapadre> parentList, int parent = 0)
    {

        <!--PARA PRODUCTOS -->
<div id="accordion">

    @foreach (var objeto in parentList)
    {
        if (objeto.id_resource == 95)
        {


            <div style="background-color:#364c66"><h5 style="text-align:center;color:#fff">@objeto.fvalueText</h5></div>

        }


        if (objeto.id_resource == 3)
        {

            <div class="card">
                <div class="card-header" id="heading_@objeto.idkey">
                    <h5 class="mb-0">
                        <button class="btn btn-link" data-toggle="collapse" data-target="#collapse_@objeto.idkey" aria-expanded="true" aria-controls="collapse_@objeto.idkey">
                            @objeto.fdescription
                        </button>
                    </h5>
                </div>

                <div id="collapse_@objeto.idkey" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                    <div class="card-body">
                        @if (objeto.children != null)
                        {
                            @OrderedList(objeto.children, objeto.idkey)
                        }
                    </div>
                </div>
            </div>
        }

    }
</div>
}



@helper OrderedList(IList<LimenawebApp.Controllers.CommercialController.MyObj_tablapadre> parentList, int parent = 0)
    {
        int barcodec = 0;

        int countbar = parentList.Where(x => x.id_resource == 28).Count();
        foreach (var objeto in parentList)
        {


            <!--PARA TITULOS -->
            @*if (objeto.id_resource == 1)
                {
                    <div class="custom-title-wrap bar-turquoise">
                        <div class="custom-title"><h2 style="text-align:center;"><small class="text-muted">@objeto.fsource</small></h2></div>
                    </div>
                }*@

            <!--PARA PRODUCTOS DINAMICOS-->
            if (objeto.id_resource == 11)
            {


                <div class="form-group">
                    <p class="control-label ">@objeto.fdescription</p>
                    <div class="">
                        @if (ViewBag.mostrarboton == 0)
                        {
                            <input type="button" onClick="cargarproductos(this.id)" value="Load products" class="btn btn-primary form-control" id="@objeto.idkey" hidden />
       
                                @OrderedListRetail(objeto.children)
                     
                        }
                        else
                        {
                            <input type="button" disabled onClick="cargarproductos(this.id)" value="Load products" class="btn btn-primary form-control btloadprod" id="@objeto.idkey" />
                        }

                    </div>
                </div>


            }
            <!--PARA CATEGORIES-->
            if (objeto.id_resource == 30)
            {

                <div class="form-group">
                    <p class="control-label "> @objeto.fsource </p>
                    <div class="">
                        <select id="cat_list" class="form-control" name="@objeto.idkey" onchange="getSubcategories()" style="width:100%" disabled>
                            <option value="0">Seleccione una categoria</option>
                        </select>

                    </div>
                </div>


            }
            <!--PARA SUBCATEGORIA-->
            if (objeto.id_resource == 31)
            {

                <div class="form-group">
                    <p class="control-label "> @objeto.fsource </p>
                    <div class="">
                        <select id="subcat_list" class="form-control" name="@objeto.idkey" onchange="getBrands()" style="width:100%" disabled>
                            <option value="0">Seleccione una subcategoria</option>
                        </select>

                    </div>
                </div>


            }

            <!--PARA SUBTITULOS -->
            if (objeto.id_resource == 8)
            {
                if (objeto.children.Count > 0)
                {
                    if (objeto.children[0].id_resource == 19)
                    {

                        <div class="form-group">
                            <p class="control-label">@objeto.fsource</p>
                            <!--VERIFICAMOS SI TIENE HIJOS-->
                            <div class="parentdiv flexRadio row">
                                @OrderedList(objeto.children, objeto.idkey)
                            </div>
                        </div>

                    }
                    else
                    {

                        <div class="form-group">
                            <p class="control-label">@objeto.fsource</p>
                            <!--VERIFICAMOS SI TIENE HIJOS-->
                            <div class="parentdiv">
                                @OrderedList(objeto.children, objeto.idkey)
                            </div>
                        </div>

                    }
                }
                else
                {
                    <div class="row">
                        <div class="col-md-12 grid-margin">
                            <hr />
                            <h4 style="text-align:center;">@objeto.fsource</h4>
                            <hr />
                        </div>
                    </div>
                }


            }



            <!--PARA PRODUCTOS -->
            if (objeto.id_resource == 3)
            {
                if (ViewBag.dinamicos == 0)
                {

                    <div class="form-group">
                        <p class="control-label ">@objeto.fdescription </p>
                        <div class="">
                            <input type="number" onClick="this.select();" min="0" inputmode="numeric" pattern="[0-9]*" value="@objeto.fvalue" class="form-control number" id="@objeto.idkey" />
                        </div>
                    </div>

                }


            }

            <!--PARA MUESTRAS -->
            if (objeto.id_resource == 4)
            {

                <div class="form-group">
                    <p class="control-label ">@objeto.fdescription </p>
                    <div class="">
                        <input type="number" onClick="this.select();" min="0" inputmode="numeric" pattern="[0-9]*" value="@objeto.fvalue" class="form-control number" id="@objeto.idkey" />
                    </div>
                </div>

            }
            <!--PARA GIFT -->
            if (objeto.id_resource == 10)
            {

                <div class="form-group">
                    <p class="control-label ">@objeto.fdescription </p>
                    <div class="">
                        <input type="number" min="0" inputmode="numeric" pattern="[0-9]*" value="@objeto.fvalue" class="form-control number" id="@objeto.idkey" required />
                    </div>
                </div>

            }
            <!--RADIO SELECCION UNICA -->
            if (objeto.id_resource == 19)
            {
                if (objeto.parent != 0)
                {

                    <div class="form-radio hRadio col-6">
                            <label class="radio-inline">
                               @if (objeto.fvalue == 0)
                            {
                                <input type="radio" class="checknew" name="@("radio_" + objeto.parent)" id="@objeto.idkey" value="@objeto.fsource" />
                            }
                            else
                            {
                                <input type="radio" class="checknew" name="@("radio_" + objeto.parent)" id="@objeto.idkey" value="@objeto.fsource" checked />
                            }
                            @objeto.fsource
                            </label>

                    </div>
                    if (objeto.children.Count > 0)
                    {
                        <div class="parentdiv fRadio col-12">
                            @if (objeto.fvalue == 0)
                            {
                                <div class="@("childdivradio_" + objeto.parent)" id="@("radiodiv_" + objeto.idkey)" style="display:none;">
                                    <hr />
                                    @OrderedList(objeto.children, objeto.idkey)
                                </div>
                            }
                            else
                            {
                                <div class="@("childdivradio_" + objeto.parent)" id="@("radiodiv_" + objeto.idkey)">
                                    <hr />
                                    @OrderedList(objeto.children, objeto.idkey)
                                </div>
                            }
                        </div>
                    }


                }
                else
                {

                    <div class="form-group">
                       <label class="radio-inline">
                                @if (objeto.fvalue == 0)
                                {
                                    <input type="radio" class="checknew" name="@("radio_" + parent)" id="@objeto.idkey" value="@objeto.fsource">
                                }
                                else
                                {
                                    <input type="radio" class="checknew" name="@("radio_" + parent)" id="@objeto.idkey" value="@objeto.fsource" checked>
                                }
                                @objeto.fsource
                         </label>
                    </div>

                }
            }
            <!--CHECK SELECCION MULTIPLE -->
            if (objeto.id_resource == 16)
            {
                if (objeto.parent != 0)
                {


                    <div class="form-check form-check-inline">
                        
                            @if (objeto.fvalue == 0)
                            {
                                <input type="checkbox" class="form-check-input chk" name="@("radio_" + objeto.parent)" id="@objeto.idkey">
                            }
                            else
                            {
                                <input type="checkbox" class="form-check-input chk" name="@("radio_" + objeto.parent)" id="@objeto.idkey" checked>
                            }
                        <label class="form-check-label" style="margin-left: 5px;" for="@objeto.idkey">
                            @objeto.fsource
                        </label>
                    </div>

                    if (objeto.children.Count > 0)
                    {
                        <div class=" parentdiv">
                            @if (objeto.fvalue == 0)
                            {
                                <div class="@("childdivradio_" + objeto.parent)" id="@("radiodiv_" + objeto.idkey)" style="display:none;">
                                    <hr />
                                    @OrderedList(objeto.children, objeto.idkey)
                                </div>
                            }
                            else
                            {
                                <div class="@("childdivradio_" + objeto.parent)" id="@("radiodiv_" + objeto.idkey)">
                                    <hr />
                                    @OrderedList(objeto.children, objeto.idkey)
                                </div>
                            }
                        </div>
                    }
                }
                else
                {

                    <!--card-body-->
                    <div class="form-group form-check-inline">
                        <div class="form-check">

                            @if (objeto.fvalue == 0)
    {
                        <input type="checkbox" class="form-check-input chk" id="@objeto.idkey">
}
else
{
                        
                                <input type="checkbox" class="form-check-input chk" id="@objeto.idkey" checked>
}   
                            <label class="form-check-label" style="margin-left: 5px;" for="@objeto.idkey">
                                @objeto.fsource
                            </label>
                        </div>

                    </div>

                }
            }

            <!--PARA BARDCODE SCANNER -->
            if (objeto.id_resource == 28)
            {
                if (barcodec == 0)
                {
                    barcodec += 1;
                    <div class="card" style="border:none !important;">

                        <div class="" id="barcodecontainer">
                            <div class="form-group codec original">

                                <div class="">
                                    <div class="input-outer">
                                        <div class="clear">X</div>

                                        <div class="input-group">
                                            <input id="scanner_input_@objeto.idkey" style="font-size:11px;" class="form-control code" placeholder="Click the button to scan..." value="@objeto.fsource" disabled type="text" />
                                            <span class="input-group-btn">
                                                <button class="btn btn-info codebtn" type="button" data-toggle="modal" id="@objeto.idkey" data-target="#livestream_scanner">
                                                    <i class="fa fa-barcode"></i>
                                                </button>
                                            </span>
                                        </div><!-- /input-group -->
                                    </div>


                                </div>

                            </div>

                        </div>
                        @if (barcodec == countbar)
                        {
                            <button class="btn btn-sm btn-outline-success" id="copyContainer" onClick="nuevoBarcode(this.id)">ADD NEW +</button>
                        }

                    </div>


                }
                else
                {
                    barcodec += 1;
                    <div class="card" style="border:none !important;">
                        <div class="" id="barcodecontainer">
                            <div class="form-group codec">

                                <div class="">
                                    <div class="input-outer">
                                        <div class="clear">X</div>

                                        <div class="input-group">
                                            <input id="scanner_input_@objeto.idkey" style="font-size:11px;" class="form-control code" placeholder="Click the button to scan..." value="@objeto.fsource" disabled type="text" />
                                            <span class="input-group-btn">
                                                <button class="btn btn-info codebtn" type="button" data-toggle="modal" id="@objeto.idkey" data-target="#livestream_scanner">
                                                    <i class="fa fa-barcode"></i>
                                                </button>
                                            </span>
                                        </div><!-- /input-group -->
                                    </div>


                                </div>

                            </div>

                        </div>
                        @if (barcodec == countbar)
                        {
                            <button class="btn btn-sm btn-outline-success" id="copyContainer" onClick="nuevoBarcode(this.id)">ADD NEW +</button>
                        }
                    </div>
                }


            }
            <!--PARA IMAGENES DE REFERENCIA -->
            if (objeto.id_resource == 29)
            {



                <p class="control-label ">@objeto.fdescription</p>
                <div class="">

                    <img class="form-control imgref1" id="@objeto.idkey">


                </div>






            }
            <!--PARA IMAGENES -->
            if (objeto.id_resource == 5 && objeto.obj_group == 0)
            {
                if (objeto.parent != 0)
                {


                    <p class="control-label ">@objeto.fdescription</p>
                    <div class="">
                        @if (objeto.fsource != "")
                        {
                            <input type="file" data-changeflag="0" class="dropifyIMG" id="@objeto.idkey" data-default-file="@Url.Content(objeto.fsource)" data-max-file-size-preview="15M">
                            <div class="progress mb-4" id="@("maindivprogress_" + objeto.idkey)" style="display:none;">
                                <div class="progress-bar bg-info" id="@("divprogress_" + objeto.idkey)" role="progressbar" style="width: 0%;" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">0%</div>
                            </div>
                            <img id="@("img_" + objeto.idkey)" alt="your image" hidden />
                            <img id="@("resultimg_" + objeto.idkey)" alt="your image" hidden />
                            <br />
                        }
                        else
                        {
                            <input type="file" data-changeflag="0" class="dropifyIMG" id="@objeto.idkey" data-max-file-size-preview="15M">
                            <div class="progress mb-4" id="@("maindivprogress_" + objeto.idkey)" style="display:none;">
                                <div class="progress-bar bg-info" id="@("divprogress_" + objeto.idkey)" role="progressbar" style="width: 0%;" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">0%</div>
                            </div>
                            <img id="@("img_" + objeto.idkey)" alt="your image" hidden />
                            <img id="@("resultimg_" + objeto.idkey)" alt="your image" hidden />
                             <br />
                        }
                    </div>


                }
                else
                {

                    <p class="control-label ">@objeto.fdescription</p>
                    <div class="">
                        @if (objeto.fsource != "")
                        {
                            <input type="file" data-changeflag="0" class="dropifyIMG" id="@objeto.idkey" data-default-file="@Url.Content(objeto.fsource)" data-max-file-size-preview="15M">
                            <div class="progress mb-4" id="@("maindivprogress_" + objeto.idkey)" style="display:none;">
                                <div class="progress-bar bg-info" id="@("divprogress_" + objeto.idkey)" role="progressbar" style="width: 0%;" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">0%</div>
                            </div>
                            <img id="@("img_" + objeto.idkey)" alt="your image" hidden />
                            <img id="@("resultimg_" + objeto.idkey)" alt="your image" hidden />
                             <br />
                        }
                        else
                        {
                            <input type="file" data-changeflag="0" class="dropifyIMG" id="@objeto.idkey" data-max-file-size-preview="15M">
                            <div class="progress mb-4" id="@("maindivprogress_" + objeto.idkey)" style="display:none;">
                                <div class="progress-bar bg-info" id="@("divprogress_" + objeto.idkey)" role="progressbar" style="width: 0%;" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">0%</div>
                            </div>
                            <img id="@("img_" + objeto.idkey)" alt="your image" hidden />
                            <img id="@("resultimg_" + objeto.idkey)" alt="your image" hidden />
                             <br />
                        }
                    </div>


                }



            }

            <!--PARA INPUT TEXT -->
            if (objeto.id_resource == 6)
            {
          
                <div class="form-group">
                     <br />
                    @if (objeto.fvalue == 0)
                    {
                        <p class="control-label">@objeto.fdescription </p>
                    }
                    else if (objeto.fvalue == 1)
                    {
                        <p class="control-label ">Flavor - @objeto.fdescription </p>
                    }
                    else if (objeto.fvalue == 2)
                    {
                        <p class="control-label ">Quality - @objeto.fdescription </p>
                    }
                    else if (objeto.fvalue == 3)
                    {
                        <p class="control-label ">Price - @objeto.fdescription </p>
                    }
                    else if (objeto.fvalue == 4)
                    {
                        <p class="control-label ">Packing - @objeto.fdescription </p>
                    }
                    else if (objeto.fvalue == 5)
                    {
                        <p class="control-label">Free - @objeto.fdescription </p>
                    }
                    <div class="">
                        <textarea id="@objeto.idkey" class="form-control" onkeypress='return ((event.charCode >= 65 && event.charCode <= 90) || (event.charCode >= 97 && event.charCode <= 122) || (event.charCode == 32))'>@objeto.fsource</textarea>
                    </div>
                </div>


            }
            <!--PARA DATE -->
            if (objeto.id_resource == 22)
            {
                if (objeto.parent != 0)
                {

                    <div class="form-group">
                        <p class="control-label"> @objeto.fsource </p>
                        <div class="">
                            @if (objeto.fvalueText == "")
                            {
                                <input type="date" id="@objeto.idkey" name="@objeto.idkey" class="form-control" value="@objeto.fvalueText" />
                            }
                            else
                            {
                                <input type="date" id="@objeto.idkey" name="@objeto.idkey" class="form-control" value="@Convert.ToDateTime(objeto.fvalueText).ToString("yyyy-MM-dd")" />
                            }

                        </div>
                    </div>

                }
                else
                {

                    <div class="form-group">
                        <p class="control-label "> @objeto.fsource </p>
                        <div class="">
                            @if (objeto.fvalueText == "")
                            {
                                <input type="date" id="@objeto.idkey" name="@objeto.idkey" class="form-control" value="@objeto.fvalueText" />
                            }
                            else
                            {
                                <input type="date" id="@objeto.idkey" name="@objeto.idkey" class="form-control" value="@Convert.ToDateTime(objeto.fvalueText).ToString("yyyy-MM-dd")" />
                            }

                        </div>
                    </div>

                }


            }


            <!--PARA INPUT NUMBER -->
            if (objeto.id_resource == 18)
            {
                if (objeto.parent != 0)
                {

                    <div class="form-group">
                        <p class="control-label "> @objeto.fsource </p>
                        <div class="">
                            <input type="number" id="@objeto.idkey" name="@objeto.idkey" class="form-control number" value="@objeto.fvalueDecimal" />

                        </div>
                    </div>

                }
                else
                {

                    <div class="form-group">
                        <p class="control-label "> @objeto.fsource </p>
                        <div class="">
                            <input type="number" id="@objeto.idkey" name="@objeto.idkey" class="form-control number" value="@objeto.fvalueDecimal" />

                        </div>
                    </div>

                }


            }
            <!--PARA INPUT NUMBER (CURRENCY)-->
            if (objeto.id_resource == 21)
            {

                <div class="form-group">
                    <p class="control-label "> @objeto.fsource </p>
                    <div class="">
                        <input type="number" id="@objeto.idkey" name="@objeto.idkey" class="form-control currency number" value="@objeto.fvalueDecimal" />

                    </div>
                </div>


            }
            <!--PARA CUSTOMERS -->
            if (objeto.id_resource == 12)
            {

                <div class="form-group" style="display:none;">
                    <p class="control-label "> @objeto.fsource </p>
                    <div class="">
                        <select id="customer_list" class="form-control" name="@objeto.idkey" style="width:100%" onChange='getCategories()'>
                            @if (ViewBag.customers.Count == 1)
                            {
                                <option value="@ViewBag.customers[0].CardCode" selected>@ViewBag.customers[0].CardCode - @ViewBag.customers[0].CardName</option>
                            }
                            else
                            {
                                <option value="0">Seleccione un cliente</option>
                                foreach (var itemCustomer in ViewBag.customers)
                                {
                                    if (objeto.fvalueText == itemCustomer.CardCode)
                                    {
                                        <option value="@itemCustomer.CardCode" selected>@itemCustomer.CardCode - @itemCustomer.CardName</option>
                                    }
                                    else
                                    {
                                        <option value="@itemCustomer.CardCode">@itemCustomer.CardCode - @itemCustomer.CardName</option>
                                    }


                                }
                            }
                        </select>

                    </div>
                </div>


            }
            <!--PARA BRANDS-->
            if (objeto.id_resource == 13)
            {

                <div class="form-group">
                    <p class="control-label "> @objeto.fsource </p>
                    <div class="">
                        <select id="brands_list" class="form-control" name="@objeto.idkey" style="width:100%" onChange='getProductslineorRefimg()' disabled>
                            <option value="0">Seleccione una marca</option>
                        </select>

                    </div>
                </div>


            }
            <!--PARA PRODUCT LINE -->
            if (objeto.id_resource == 14)
            {

                <div class="form-group">
                    <p class="control-label "> @objeto.fsource </p>
                    <div class="">
                        <select id="productline_list" name="@objeto.idkey" class="form-control" style="width:100%">
                            <option value="0">Seleccione un linea de producto</option>
                        </select>

                    </div>
                </div>


            }
            <!--PARA SELECT GENERAL -->
            if (objeto.id_resource == 17)
            {

                <div class="form-group">
                    <p class="control-label "> @objeto.fsource </p>
                    <div class="">
                        <select id="@objeto.idkey" name="@objeto.idkey" class="form-control" style="width:100%">
                            <option value="0">Select an item</option>
                            @if (objeto.fsource == "Cuantas caras tiene la categoria en general")
                            {

                                for (var i = 1; i < 500; i++)
                                {
                                                        if (objeto.fvalueText != "")
                                    {
                                        if (Convert.ToInt32(objeto.fvalueText) == i) {  <option value="@i" selected>@i</option> } else {  <option value="@i">@i</option> }
                                    }
                                    else {

                                        <option value="@i">@i</option>
                                    }
                                }
                            }
                            @if (objeto.fsource == "Cuantas marcas estan en la misma categoria")
                            {

                                for (var i = 1; i < 500; i++)
                                {
                                                       if (objeto.fvalueText != "")
                                    {
                                        if (Convert.ToInt32(objeto.fvalueText) == i) {  <option value="@i" selected>@i</option> } else {  <option value="@i">@i</option> }
                                    }
                                    else {

                                        <option value="@i">@i</option>
                                    }
                                }
                            }                     
                            @if (objeto.fsource == "¿Cuantas caras tiene el producto/sku?")
                            {

                                for (var i = 1; i < 500; i++)
                                {
                                                       if (objeto.fvalueText != "")
                                    {
                                        if (Convert.ToInt32(objeto.fvalueText) == i) {  <option value="@i" selected>@i</option> } else {  <option value="@i">@i</option> }
                                    }
                                    else {

                                        <option value="@i">@i</option>
                                    }
                                }
                            }



                        </select>

                    </div>
                </div>


            }
            <!--PARA ITEMS DE SELECT GENERAL -->
            if (objeto.id_resource == 20)
            {
                IList<LimenawebApp.Models.FormsM_details> mainlist = ViewBag.detailssql;

                var idparent = mainlist.Where(c => c.idkey == objeto.parent).FirstOrDefault();

                if (idparent != null)
                {
                    if (idparent.ID_formresourcetype == 17)
                    {
                        if (idparent.fvalueText == objeto.idkey.ToString())
                        {
                            <option value="@objeto.idkey" selected>@objeto.fsource</option>
                        }
                        else
                        {
                            <option value="@objeto.idkey">@objeto.fsource</option>
                        }
                    }
                }
                else
                {
                    <option value="@objeto.idkey">@objeto.fsource</option>

                }




            }

            <!--PARA BRAND COMPETITORS -->
            if (objeto.id_resource == 15)
            {

                <div class="form-group">
                    <p class="control-label "> @objeto.fsource </p>
                    <div class="">
                        <select class="form-control" id="brandcompetitors_list" name="@objeto.idkey" style="width:100%">
                            <option value="0">Seleccione competidor de la marca</option>
                        </select>

