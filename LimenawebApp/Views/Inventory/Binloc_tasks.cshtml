                    $.ajax
                        ({
                            url: '/Commercial/GetUOM',
                            type: 'POST',
                            datatype: 'application/json',
                            contentType: 'application/json',
                            data: JSON.stringify({
                                product: IDProd
                            }),
                            success: function (result) {
                                $("#UOMlst").html("");
                                $("#UOMlst").append($('<option></option>').val(0).html("Select an UoM"));
