
@{
    ViewBag.Title = "Distribuidora Limeña - Líderes en productos latinos";
}

<div class="maincontent-services">
    <div class="header">
        <h1 class="h1-subpage-title white">Contáctanos</h1>
    </div>
</div>
<div class="contact-container">
    <div id="map" class="map w-widget w-widget-map"></div>

    <div class="form-block-2 w-form">
        <form id="email-form" name="email-form" data-name="Email Form" class="form-2">
            <h3>Ponte en contacto</h3><input type="text" class="w-input" maxlength="256" name="Nombre" data-name="Nombre" placeholder="Nombre" id="Nombre" required=""><input type="text" class="w-input" maxlength="256" name="Empresa" data-name="Empresa" placeholder="Empresa" id="Empresa" required=""><input type="text" class="w-input" maxlength="256" name="email" data-name="Email" placeholder="Correo electrónico" id="email" required=""><textarea id="Mensaje" name="Mensaje" data-name="Mensaje" maxlength="5000" placeholder="Escribe tu mensaje aquí" required="" class="w-input"></textarea><input type="submit" value="Enviar" data-wait="Please wait..." class="formbutton w-button">
        </form>
        <div class="w-form-done">
            <div>Thank you! Your submission has been received!</div>
        </div>
        <div class="w-form-fail">
            <div>Oops! Something went wrong while submitting the form.</div>
        </div>
    </div>
</div>

